{
    "name": "ammaar23/postcodes-io-sdk",
    "description": "A simple PHP sdk for Postcodes.io",
    "keywords": ["postcodes", "package", "sdk", "php", "postcodes.io"],
    "homepage": "https://github.com/ammaar23/postcodes-io-sdk",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ammaar Latif",
            "email": "ammaarlatif_14@hotmail.co.uk"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Ammaar23\\Postcodes\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ],
        "test-coverage": [
            "vendor/bin/phpunit --coverage-html tests/coverage-report"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
